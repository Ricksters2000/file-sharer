{
  "name": "file-sharer",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "setup": "nvm use 18.16.0",
    "dev": "nodemon",
    "start": "node ./.dev/server.dev.js",
    "build-dev-server": "tsc && vite build --mode development --config ./build/server.config.dev.ts",
    "build-dev-client": "tsc && vite build --mode development --config ./build/client.config.dev.ts",
    "build-dev-server-once": "tsc && vite build --mode production --watch false --config ./build/server.config.dev.ts",
    "build-dev-client-once": "tsc && vite build --watch false --config ./build/client.config.dev.ts",
    "build-and-start": "npm run build-dev-server-once && npm run build-dev-client-once && npm run start",
    "build": "tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "postinstall": "patch-package"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.353.0",
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "formidable": "^2.1.2",
    "patch-package": "^7.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.2.3",
    "react-router-dom": "^6.11.2",
    "s3rver": "^3.7.1",
    "tailwindcss": "^3.3.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.11.0",
    "@types/express": "^4.17.17",
    "@types/formidable": "^2.0.6",
    "@types/node": "^20.2.5",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/s3rver": "^3.7.0",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react": "^4.0.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "nodemon": "^2.0.22",
    "rollup": "^3.23.0",
    "typescript": "^5.0.2",
    "vite": "^4.3.2"
  }
}
